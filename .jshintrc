{
	// Wikia Mercury .jshintrc
	// Please keep these in alphabetical order.
	// Installation instructions and linters for your preferred
	// IDE/editor here: http://www.jshint.com/install/

	// Enforcing

	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"eqeqeq": true,
	"immed": true,
	"latedef": false,
	"maxdepth": 3,
	"maxlen": 120,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonew": true,
	"onevar": true,
	"plusplus": false,
	"quotmark": "single",
	"undef": true,
	"unused": true,
	"strict": false,
	"trailing": true,

	// Relaxing
	"asi": false,
	"boss": false,
	"debug": false,
	"evil": false,
	"expr": false,
	"funcscope": false,
	"globalstrict": false,
	"iterator": false,
	"lastsemic": false,
	"laxbreak": false,
	"laxcomma": false,
	"loopfunc": false,
	"moz": false,
	"multistr": false,
	"proto": false,
	"scripturl": false,
	"shadow": false,
	"smarttabs": false,
	"sub": false,
	"supernew": false,
	"validthis": false,

	// Environments
	"browser": true,
	"devel": false,
	"globals": {
		"App": false,
		"Em": false,
		"Ember": false,
		"Mercury": false,
		"QUnit": false,
		"W": false,
		"Wikia": false,
		"andThen": false,
		"deepEqual": false,
		"emq": false,
		"equal": false,
		"expect": false,
		"find": false,
		"moduleFor": false,
		"moduleForComponent": false,
		"ok": false,
		"setResolver": false,
		"strictEqual": false,
		"test": false,
		"throws": false,
		"visit": false
	},
	"jquery": true,
	"node": true,
	"nonstandard": true,
	"predef": [
	  "document",
	  "window",
	  "-Promise"
	],

	"curly": true,
	"debug": false,
	"devel": true,
	"eqeqeq": true,
  "eqnull": true,
	"evil": true,
	"forin": false,
	"immed": false,
	"laxbreak": false,
	"newcap": true,
	"noarg": true,
	"noempty": false,
	"nonew": false,
	"nomen": false,
	"onevar": false,
	"plusplus": false,
	"undef": true,
	"sub": true,
	"strict": false,
	"white": false,
	"eqnull": true,
	"esnext": true
}
