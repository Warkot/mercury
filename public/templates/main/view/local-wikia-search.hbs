<ul>
	{{!-- {{#unless controller.suggestions}}
		<li>{{unbound emptyMessage}}<li>
	{{/unless}} --}}
	{{#each controller.suggestions}}
		<a {{action collapseSideNav target="view.parentView" on="mouseUp"}} href="{{unbound url}}">
			<li>{{unbound title}}</li>
		</a>
	{{else}}
		{{#if emptyMessage}}
			<li>{{unbound emptyMessage}}</li>
		{{/if}}
	{{/each}}
</ul>