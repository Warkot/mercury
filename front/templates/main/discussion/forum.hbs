{{discussion-header
	setSortBy='setSortBy'
	sortBy=sortBy
}}
{{discussion-rail
	railItems=leftRailItems
	isLeft=true
	threeColumn=true
}}
{{discussion-rail
	railItems=rightRailItems
	isRight=true
	threeColumn=true
	goToMainPage='goToMainPage'
}}
{{forum-wrapper
	goToPost='goToPost'
	loadPage='loadPage'
	name=model.name
	posts=model.posts
	postsDisplayed=model.posts.length
	totalPosts=model.totalPosts
}}

