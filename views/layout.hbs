<!DOCTYPE html>
<html class="no-js">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>{{article.cleanTitle}} - {{siteName}}</title>
		{{#if article.description}}
		<meta name="description" content="{{article.description}}">
		{{/if}}
		<meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">

		<!-- build:css /public/styles/app.css -->
		<link rel="stylesheet" href="/public/styles/app.css">
		<!-- endbuild -->

		<style>
			<!-- @include ../www/public/styles/baseline.css -->
		</style>

		<script>
			<!-- @include ../www/public/scripts/baseline.js -->
			W.provide('_t.{{#if language}}{{language}}{{else}}en{{/if}}', {{{translations language}}});
			W.provide('_state.firstPage', true);
			W.provide('ads.slots', []);
            W.provide('apiBase', '{{{apiBase}}}');
			{{#if wikiJson}}
                W.provide('wiki', {{{wikiJson}}});
			{{/if}}

			{{#if articleJson}}
				W.provide('article', {{{articleJson}}});
			{{else}}
				W.provide('article', {});
				<!--W.provide('error.message', '{{message}}');-->
				<!--W.provide('error.status', '{{code}}');-->
				<!--W.provide('error.error', '{{details}}');-->
			{{/if}}
		</script>

		<!-- Google Analytics -->
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			// staging GA id
			ga('create', '{{gaId}}', 'auto');
			ga('send', 'pageview');
		</script>
		<!-- End Google Analytics -->

		<!-- @ifdef browserSync -->
		<script src="/public/scripts/dev.js"></script>
		<!-- @endif -->
	</head>
	<body>
		<!-- @include ../www/public/svg/main.svg -->

		<div id="article-preload">
			{{{content}}}
		</div>

		<!-- build:js /public/vendor/main.js -->
			<script src="/public/vendor/script.js/dist/script.js"></script>
			<script src="/public/vendor/jquery/dist/jquery.js"></script>
			<script src="/public/vendor/hammerjs/hammer.js"></script>
			<script src="/public/vendor/handlebars/handlebars.runtime.js"></script>
			<script src="/public/vendor/ember/ember.js"></script>
			<script src="/public/vendor/ember-hammer/ember-hammer.js"></script>
			<script src="/public/vendor/i18next/i18next.js"></script>
		<!-- @if WIKIA_ENVIRONMENT='production' -->
		<!-- @endif -->
		<!-- endbuild -->

		<!-- build:js /public/scripts/combined.js -->
			<script src="/public/templates/main.js"></script>
			<script src="/public/scripts/wikia.js"></script>
			<script src="/public/scripts/main.js"></script>
		<!-- endbuild -->

		{{#if mediawikiDomain}}
		<script async src="{{mediawikiDomain}}__am/{{cacheBuster}}/groups/-/mercury_ads_js"></script>
		{{/if}}
	</body>
</html>
